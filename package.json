  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview --port 5050",
    "typecheck": "vue-tsc --noEmit"
  },
  "dependencies": {
    "vue": "^3.2.33"
  },
  "devDependencies": {
    "@types/node": "^16.11.27",
    "@vitejs/plugin-vue": "^2.3.1",
    "@vue/tsconfig": "^0.1.3",
    "postcss": "^8.4.13",
    "postcss-dark-theme-class": "^0.7.3",
    "postcss-nested": "^5.0.6",
    "typescript": "~4.6.3",
    "vite": "^2.9.5",
    "vue-tsc": "^0.34.7"
  },
  "postcss": {
    "plugins": {
      "postcss-nested": {},
      "postcss-dark-theme-class": {
        "darkSelector": ".dark",
        "lightSelector": ".light"
      }
    }
  }
